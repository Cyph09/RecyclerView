<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.swaleh.expandedreportsedairy">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_icon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SelectCriteriaActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CenterReportSummaryActivity"
            android:parentActivityName=".SelectCriteriaActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".SelectCriteriaActivity" />
        </activity>
        <activity android:name=".IndividualCenterReportSummary"></activity>
    </application>

</manifest>